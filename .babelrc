{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": 3,
        "modules": false,
        "loose": true,
        "useBuiltIns": "usage",
        "targets": {
          "browsers": ["last 3 versions", "ie >= 11"]
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
