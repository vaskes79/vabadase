{
  "name": "vabadase",
  "version": "0.2.0",
  "description": "landing pages row house",
  "main": "index.js",
  "repository": "git@github.com:vaskes79/vabadase.git",
  "author": "Vasily Guzov <vasily.guzov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "stage:server": "cd public && npx browser-sync start --server --no-notify --no-ui --files *",
    "prestage:server": "yarn build",
    "prebuild": "rm -r .cache-loader/*",
    "build": "NODE_ENV=production webpack --mode=production",
    "postbuild": "yarn copy:files",
    "copy:files": "cp ./src/robots.txt ./src/.htaccess ./src/*.png ./src/browserconfig.xml ./src/site.webmanifest ./src/safari*.svg ./src/favicon.ico ./public",
    "dev": "webpack-dev-server",
    "predeploy": "yarn run build",
    "fix-styles": "prettier-stylelint --write 'src/**/*.{css,scss}'",
    "deploy": "gh-pages -d public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cache-loader": "^2.0.1",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.7.1",
    "jest-css-modules": "^2.0.0",
    "jest-extended": "^0.11.1",
    "jimp": "^0.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^1.10.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "responsive-loader": "^1.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "core-js": "3",
    "google-maps": "^3.3.0"
  }
}
