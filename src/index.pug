-const {
-    head: {title}, 
-    nav, 
-    header, 
-    slideShow,
-    skills,
-    gallery,
-    price, 
-    plans, 
-    twitter, 
-    contacts, 
-  } = require('./data.json');

doctype html
html(lang='ru')
  head
    meta(charset='utf-8')
    meta(http-equiv='x-ua-compatible', content='ie=edge')
    meta(http-equiv='Content-Type', content='text/html; charset=utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    title= title
    //if lt IE 9
      script(src='http://html5shiv.googlecode.com/svn/trunk/html5.js')
  body
    include static/icon/symbol-defs.svg
    include parts/mixins
    include parts/nav
    include parts/header
    main.main
      include parts/slide-show
      include parts/skills
      include parts/gallery
      include parts/price
      include parts/plans
      include parts/twitter
